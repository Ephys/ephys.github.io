{
  "name": "@ephys/ephys.github.io",
  "private": true,
  "description": "My portfolio",
  "version": "2.0.0",
  "author": "Guylian Cox <guylian.cox@gmail.com>",
  "scripts": {
    "start": "gatsby serve",
    "build": "NODE_ENV=production gatsby build && cp CNAME public/CNAME",
    "start:dev": "gatsby develop",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "serve": "gatsby serve",
    "test": "check-engines && concurrently \"npm:test:*\"",
    "test:ts-dry-run": "check-engines && tsc --noEmit --project tsconfig.json --skipLibCheck",
    "release": "npm run build && gh-pages --branch \"gh-pages\" -d public --message \"[skip ci] Deploy\" --dotfiles"
  },
  "dependencies": {
    "add": "^2.0.6",
    "classnames": "^2.2.6",
    "gatsby": "^2.24.63",
    "gatsby-image": "^2.4.19",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-manifest": "^2.4.30",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "gatsby-plugin-sass": "^2.3.13",
    "gatsby-plugin-sharp": "^2.6.36",
    "gatsby-plugin-typescript": "^2.4.20",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-source-name": "^1.0.0",
    "gatsby-source-filesystem": "^2.3.30",
    "gatsby-transformer-remark": "^2.8.35",
    "gatsby-transformer-sharp": "^2.5.15",
    "http-server": "^0.12.3",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.26.11"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "check-engines": "^1.5.0",
    "concurrently": "^5.3.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.1.2",
    "react-feather": "^2.0.8",
    "typescript": "^4.0.3",
    "typescript-plugin-css-modules": "^2.7.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ephys/ephys.github.io"
  },
  "bugs": {
    "url": "https://github.com/ephys/ephys.github.io/issues"
  }
}
