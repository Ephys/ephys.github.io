{
  "name": "@ephys/ephys.github.io",
  "private": true,
  "description": "My portfolio",
  "version": "2.0.0",
  "author": "Guylian Cox <guylian.cox@gmail.com>",
  "scripts": {
    "start": "gatsby serve",
    "build": "gatsby build && cp CNAME public/CNAME",
    "start:dev": "gatsby develop",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "serve": "gatsby serve",
    "test": "check-engines && concurrently \"npm:test:*\"",
    "test:ts-dry-run": "check-engines && tsc --noEmit --project tsconfig.json --skipLibCheck",
    "release": "npm run build && gh-pages --branch master -d public --message \"[skip ci] Deploy\""
  },
  "dependencies": {
    "add": "^2.0.6",
    "classnames": "^2.2.6",
    "gatsby": "^2.19.43",
    "gatsby-image": "^2.2.43",
    "gatsby-plugin-manifest": "^2.2.47",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sass": "^2.1.30",
    "gatsby-plugin-sharp": "^2.4.12",
    "gatsby-plugin-typescript": "^2.2.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-source-name": "^1.0.0",
    "gatsby-source-filesystem": "^2.1.55",
    "gatsby-transformer-remark": "^2.6.58",
    "gatsby-transformer-sharp": "^2.3.18",
    "http-server": "^0.12.1",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "check-engines": "^1.5.0",
    "concurrently": "^5.1.0",
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3",
    "typescript-plugin-css-modules": "^2.2.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ephys/ephys.github.io"
  },
  "bugs": {
    "url": "https://github.com/ephys/ephys.github.io/issues"
  }
}
